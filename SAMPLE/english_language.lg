ENGLISH_LANGUAGE : LANGUAGE

    "princess"
        1*"princess":female

    "NewGame"
        "New game"

    GameOver
        "Game over!"

    Welcome first_name last_name
        "Welcome, " ~ first_name ~ " " ~ last_name ~ "!"

    Pears #count
        $count ~ " "
        if count = 1
            "pear"
        else
            "pears"
