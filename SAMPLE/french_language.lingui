FRENCH_LANGUAGE : LANGUAGE

    "princess"
        1*"princesse":female

    "NewGame"
        "Nouveau jeu"

    GameOver
        "Fin du jeu !"

    Welcome first_name last_name
        "Bienvenue, " ~ first_name ~ " " ~ last_name ~ " !"

    Pears #count
        $count ~ " "
        if count = 0 or count = 1
            "poire"
        else
            "poires"
