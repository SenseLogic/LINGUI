FRENCH_LANGUAGE : LANGUAGE

    "princess"
        1*"princesse":female

    "NewGame"
        "Nouveau jeu"

    GameOver
        "Fin du jeu!"

    Welcome first_name last_name
        "Bienvenue, " ~ $first_name ~ " " ~ $last_name ~ " !"

    Pears count
        *count ~ " "
        if @count = one
            "poire"
        else
            "poires"
