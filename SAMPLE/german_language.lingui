GERMAN_LANGUAGE : LANGUAGE

    "princess"
        1*"Prinzessin":female

    "NewGame"
        "Neues Spiel"

    GameOver
        "Spiel vorbei!"

    Welcome first_name last_name
        "Willkommen, " ~ first_name ~ " " ~ last_name ~ "!"

    Pears #count
        $count ~ " "
        if count = 1
            "Birne"
        else
            "Birnen"
